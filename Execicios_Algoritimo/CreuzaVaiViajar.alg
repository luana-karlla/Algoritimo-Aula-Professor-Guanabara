algoritmo "conversaoValores"
// Função :
// Autor :
// Data : 05/10/2021
// Seção de Declarações 
var
     reais, dolares: Real
inicio
// Seção de Comandos 
   Escreva("Quantos reais eu tenho? R$")
   leia(reais)
   
   dolares<-reais/5.28
   
   Escreval("Posso ter em Dólares S$$", dolares)
fimalgoritmo
